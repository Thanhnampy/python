   "source": [
    "# in bảng cửu chương\n",
    "n = int(input(\"Nhập bảng nhân: \"))\n",
    "s = 0\n",
    "for i in range (0,10):\n",
    "    i+=1\n",
    "    s=n*i\n",
    "    print(f\"{n}x{i}={s}\")"
